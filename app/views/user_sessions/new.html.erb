<h1>Log In</h1>

<% form_for @user_session do |f| %>
  <%= f.error_messages :header_message => "", :message => "Problem logging in:" %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit "Log In" %></p>
<% end %>